{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Onyx Engine",
  "colors": {
    "primary": "#1F8AD7",
    "light": "#60BAFA",
    "dark": "#232729"
  },
  "styling": {
    "codeblocks": {
      "theme": {
        "light": "github-dark-dimmed",
        "dark": "github-dark-dimmed"
      }
    }
  },
  "icons": {
    "library": "lucide"
  },
  "background": {
    "decoration": "grid"
  },
  "fonts": {
    "family": "Geist",
    "weight": 400
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "icon": "rocket",
            "pages": [
              "index",
              "quickstart",
              "installation"
            ]
          },
          {
            "group": "Tutorials",
            "icon": "book-open",
            "pages": [
              "tutorials/working-with-datasets",
              "tutorials/training-models",
              "tutorials/optimizing-models",
              "tutorials/simulating-models"
            ]
          },
          {
            "group": "Concepts",
            "icon": "lightbulb",
            "pages": [
              "concepts/model-architectures",
              "concepts/feature-engineering",
              "concepts/training-configuration"
            ]
          },
          {
            "group": "Platform",
            "icon": "monitor",
            "pages": [
              "platform/overview",
              "platform/uploading-datasets",
              "platform/training-via-ui",
              "platform/viewing-results"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/index"
            ]
          },
          {
            "group": "Core Functions",
            "pages": [
              "api-reference/save-dataset",
              "api-reference/load-dataset",
              "api-reference/save-model",
              "api-reference/load-model",
              "api-reference/train-model",
              "api-reference/optimize-model"
            ]
          },
          {
            "group": "Data Classes",
            "pages": [
              "api-reference/onyx-dataset"
            ]
          },
          {
            "group": "Model Configurations",
            "pages": [
              "api-reference/mlp-config",
              "api-reference/rnn-config",
              "api-reference/transformer-config"
            ]
          },
          {
            "group": "Feature Classes",
            "pages": [
              "api-reference/input",
              "api-reference/output"
            ]
          },
          {
            "group": "Training Classes",
            "pages": [
              "api-reference/training-config",
              "api-reference/optimization-config",
              "api-reference/optimizer-configs"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Platform",
          "href": "https://engine.onyx-robotics.com",
          "icon": "globe"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/onyx-robotics/onyx-engine",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Home",
        "href": "https://onyx-robotics.com"
      },
      {
        "label": "Support",
        "href": "mailto:support@onyx-robotics.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Platform",
      "href": "https://engine.onyx-robotics.com"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/onyx-robotics"
    }
  }
